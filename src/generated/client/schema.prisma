generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHERS
}

model User {
  id          Int     @id @default(autoincrement())
  name        String
  email       String
  age         Int?
  gender      Gender?
  profession  String?
  phoneNumber String?
  createdAt   DateTime  @default(now())
}
